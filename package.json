{
  "name": "ecuador-map-editor",
  "version": "1.0.0",
  "description": "",
  "main": "app.js",
  "engine": {
    "node": "5.7.0"
  },
  "scripts": {
    "build": "npm run build:id && npm run build:bundle",
    "build:id": "node bin/build_id_editor.js",
    "build:bundle": "browserify browser/main.js -o public/bundle.js",
    "build:sync": "browserify browser/sync.js -o public/bundle_sync.js",
    "package-win": "rimraf \"dist/Mapeo CEIBO-win32-x64\" && npm run build && electron-packager . \"Mapeo CEIBO\" --out=dist/ --platform=win32 --arch=x64 --version=0.36.9 --ignore=\"^dist\" --prune --overwrite --asar",
    "package-mac": "rimraf \"dist/Mapeo CEIBO-darwin-x64\" && npm run build && electron-packager . \"Mapeo CEIBO\" --out=dist/ --platform=darwin --arch=x64 --version=0.36.9 --ignore=\"^dist\" --prune --overwrite",
    "rebuild-leveldb": "cd node_modules/leveldown && set HOME=~/.electron-gyp && node-gyp rebuild --target=0.36.9 --arch=x64 --dist-url=https://atom.io/download/atom-shell",
    "start": "electron app.js --disable-http-cache",
    "server": "node app.js --headless",
    "watch": "npm run watch:bundle & npm run watch:sync",
    "watch:bundle": "watchify browser/main.js -o public/bundle.js -dv",
    "watch:sync": "watchify browser/sync.js -o public/bundle_sync.js -dv",
    "dev": "electron app.js --debug --disable-http-cache"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/digidem/ecuador-map-editor.git"
  },
  "keywords": [],
  "author": "substack",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/digidem/ecuador-map-editor/issues"
  },
  "homepage": "https://github.com/digidem/ecuador-map-editor#readme",
  "dependencies": {
    "JSONStream": "^1.0.3",
    "body": "^5.1.0",
    "concat-stream": "^1.5.1",
    "ecstatic": "^1.4.0",
    "end-of-stream": "^1.1.0",
    "hyperlog": "^4.8.0",
    "iD": "github:openstreetmap/iD#v1.9.1",
    "level": "^1.4.0",
    "levelup": "^1.3.1",
    "minimist": "^1.2.0",
    "once": "^1.3.3",
    "osm-p2p": "^1.2.2",
    "osm-p2p-server": "^1.5.0",
    "presets-wao": "1.0.0",
    "pump": "^1.0.1",
    "randombytes": "^2.0.3",
    "shpjs": "^3.3.1",
    "split2": "^2.0.1",
    "sqldown": "^2.1.0",
    "sqlite3": "^3.1.1",
    "through2": "^2.0.1",
    "websocket-stream": "^3.1.0",
    "xhr": "^2.2.0",
    "xtend": "^4.0.1"
  },
  "devDependencies": {
    "browserify": "^13.0.0",
    "electron-packager": "^5.2.1",
    "electron-prebuilt": "0.36.9",
    "rimraf": "^2.5.2",
    "watchify": "^3.7.0"
  }
}
