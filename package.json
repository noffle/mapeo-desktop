{
  "name": "mapeo-desktop",
  "description": "An experimental offline mapping app based on iD Editor, for indigenous territory mapping in remote environments",
  "productName": "Mapeo",
  "version": "1.2.8",
  "productDescription": "Offline Map Editor",
  "author": "Digital Democracy",
  "main": "app.js",
  "engine": {
    "node": "6.2.1"
  },
  "scripts": {
    "build": "npm run build:id",
    "build:id": "mkdir -p vendor; mv $(node bin/build_id_editor.js) vendor/iD",
    "clean": "rimraf \"dist/Mapeo-win32-x64\"",
    "package-win": "node bin/build_id_editor.js | node bin/package_windows.js",
    "installer-win": "node bin/build_id_editor.js | node bin/package_windows.js | node bin/create_windows_installer.js",
    "dmg-mac": "node bin/build_id_editor.js | node bin/package_macos.js | node bin/create_macos_dmg.js",
    "package-mac": "node bin/build_id_editor.js | node bin/package_macos.js",
    "rebuild-leveldb": "cd node_modules/leveldown && cross-env HOME=~/.electron-gyp node-gyp rebuild --target=1.3.4 --runtime=electron --arch=x64 --dist-url=https://atom.io/download/atom-shell",
    "start": "electron app.js --disable-http-cache",
    "server": "node app.js --headless",
    "dev": "electron app.js --debug --disable-http-cache",
    "postinstall": "npmpd"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/digidem/mapeo-desktop.git"
  },
  "keywords": [
    "osm",
    "p2p",
    "peer-to-peer",
    "gis",
    "maps",
    "iD",
    "osm-p2p"
  ],
  "contributors": [
    "substack",
    "Gregor MacLennan <gmaclennan@digital-democracy.org>",
    "noffle"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/digidem/mapeo-desktop/issues"
  },
  "homepage": "https://github.com/digidem/mapeo-desktop#readme",
  "dependencies": {
    "JSONStream": "^1.0.3",
    "application-config": "^1.0.1",
    "application-config-path": "^0.1.0",
    "body": "^5.1.0",
    "concat-stream": "^1.5.1",
    "ecstatic": "^2.1.0",
    "electron-debug": "^1.0.0",
    "electron-squirrel-startup": "^1.0.0",
    "electron-winstaller": "^2.3.1",
    "end-of-stream": "^1.1.0",
    "hyperlog": "^4.10.0",
    "insert-css": "^1.0.0",
    "level": "^1.4.0",
    "levelup": "^1.3.1",
    "lodash": "^4.15.0",
    "minimist": "^1.2.0",
    "once": "^1.3.3",
    "osm-p2p": "^1.4.0",
    "osm-p2p-geojson": "^2.0.0",
    "osm-p2p-server": "^2.0.0-beta3",
    "pump": "^1.0.1",
    "randombytes": "^2.0.3",
    "shpjs": "^3.3.1",
    "split2": "^2.0.1",
    "sqldown": "^2.1.0",
    "sqlite3": "^3.1.1",
    "statuses": "^1.3.0",
    "svg-sprite": "^1.3.5",
    "tar-fs": "^1.13.0",
    "through2": "^2.0.1",
    "websocket-stream": "^3.1.0",
    "xhr": "^2.2.0",
    "xtend": "^4.0.1"
  },
  "darwinDependencies": {
    "appdmg": "0.4.5"
  },
  "devDependencies": {
    "@gmaclennan/concat": "^1.0.0",
    "argv-or-stdin": "0.0.1",
    "browserify": "^13.0.0",
    "cross-env": "^1.0.8",
    "cross-spawn": "^4.0.2",
    "devtron": "^1.2.0",
    "electron": "1.3.4",
    "electron-packager": "^8.1.0",
    "fs-extra": "^0.30.0",
    "iD": "openstreetmap/iD#v1.9.6",
    "ice-box": "0.1.0",
    "mkdirp": "^0.5.1",
    "npm-platform-dependencies": "0.0.12",
    "rimraf": "^2.5.2",
    "watchify": "^3.7.0"
  }
}
